<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" styleClass="instert-btn-picture"
            stylesheets="@dashboard2.css" xmlns="http://javafx.com/javafx/21"
            fx:controller="com.esprit.controllers.evenements.MeteoEventController">
    <children>
        <ImageView fx:id="img" fitHeight="150.0" fitWidth="200.0" layoutX="24.0" layoutY="104.0" pickOnBounds="true"
